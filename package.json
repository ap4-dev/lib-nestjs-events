{
  "name": "@ap-dev/nestjs-events",
  "version": "0.1.12",
  "description": "A NestJS events library with RabbitMQ integration",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "nestjs",
    "events",
    "typescript",
    "library"
  ],
  "author": "Alexander Padron alex@ap4.dev>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ap4-dev/lib-nestjs-events.git"
  },
  "bugs": {
    "url": "https://github.com/ap4-dev/lib-nestjs-events/issues"
  },
  "homepage": "https://github.com/ap4-dev/lib-nestjs-events#readme",
  "dependencies": {
    "amqplib": "^0.10.3"
  },
  "peerDependencies": {
    "@nestjs/common": "^11.0.1",
    "@nestjs/core": "^11.0.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@nestjs/testing": "^11.0.1",
    "@types/amqplib": "0.10.5",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.3.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "publishConfig": {
    "access": "public"
  }
}
